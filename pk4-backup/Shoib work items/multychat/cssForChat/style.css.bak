.glossy {
	padding:5px;
	/* margin: 8px; */
	margin-bottom: 5px;
	/*padding-top:-5px;
	padding-bottom:10px;*/
	position: relative;
	float:right;
	background: #94c4fe;
	background: -webkit-gradient(linear, left top, left bottom, color-stop(31%,#94c4fe), color-stop(100%,#d3f6fe));
	background: -webkit-linear-gradient(top,  #94c4fe 31%,#d3f6fe 100%);
	background: -moz-linear-gradient(top,  #94c4fe 31%, #d3f6fe 100%);
	background: -o-linear-gradient(top,  #94c4fe 31%,#d3f6fe 100%);
	background: -ms-linear-gradient(top,  #94c4fe 31%,#d3f6fe 100%);
	background: linear-gradient(to bottom,  #94c4fe 31%,#d3f6fe 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#94c4fe', endColorstr='#d3f6fe',GradientType=0 );

	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;

	border: 1px solid #4864a9;

	color: #000;
	font-size: 0.950em !important;
	font-family: Tahoma !important;
	

	-webkit-box-shadow:  0px 1px 3px 0px rgba(0, 0, 0, .2);    
    box-shadow:  0px 1px 3px 0px rgba(0, 0, 0, .2);

    position: relative;
	
	max-width:85%;
	white-space: pre-wrap;
	word-wrap: break-word;
}

.glossy:before {
	content: "";
	display: block;
	position: absolute;
	left: 5px;

	-webkit-border-radius: 8px;
	-moz-border-radius: 8px;
	border-radius: 8px;

	background: -moz-linear-gradient(top,  rgba(255,255,255,1) 0%, rgba(255,255,255,0.7) 8%, rgba(255,255,255,0) 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,1)), color-stop(8%,rgba(255,255,255,0.7)), color-stop(100%,rgba(255,255,255,0)));
	background: -webkit-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0.7) 8%,rgba(255,255,255,0) 100%);
	background: -o-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0.7) 8%,rgba(255,255,255,0) 100%);
	background: -ms-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0.7) 8%,rgba(255,255,255,0) 100%);
	background: linear-gradient(to bottom,  rgba(255,255,255,1) 0%,rgba(255,255,255,0.7) 8%,rgba(255,255,255,0) 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#00ffffff',GradientType=0 );
}

/*Green*/

.glossy.green {
	float:left;
	background: #54bc3e;
	background: -moz-linear-gradient(top,  #54bc3e 0%, #aee850 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#54bc3e), color-stop(100%,#aee850));
	background: -webkit-linear-gradient(top,  #54bc3e 0%,#aee850 100%);
	background: -o-linear-gradient(top,  #54bc3e 0%,#aee850 100%);
	background: -ms-linear-gradient(top,  #54bc3e 0%,#aee850 100%);
	background: linear-gradient(to bottom,  #54bc3e 0%,#aee850 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#54bc3e', endColorstr='#aee850',GradientType=0 );
	border: 1px solid #1d6511;
}

   .content {
	
        position: relative;
        background: white;
		min-height:175px;
		max-height:250px;
		overflow-y: auto;
		width: 245px;
		max-width: 245px;
		padding:5px 0px 5px 5px ;
		white-space: pre-wrap;
		word-wrap: break-word;
    }

	.client_msgWrap{
		width:98%;
		margin:0px auto;
		float:left;
		display:block;
		white-space: pre-wrap;
		word-wrap: break-word;
		font-size: 15px;
	}
	
	.clientMsg{
		max-width:90%;
		float:right;
		border-radius:5px;
		color: rgb(93, 94, 100);
		background:rgb(155, 184, 128);
		margin: 0px auto;
		margin-right: 3px;
		margin-left:3px;
		padding: 5px;
		margin-bottom:3px;
		white-space: pre-wrap;
		word-wrap: break-word;
	}
	
	.servrMsg{
		max-width:90%;
		float:left;
		border-radius:5px;
		color: rgb(93, 94, 100);
		background:rgb(236, 181, 99);
		margin: 0px auto;
		margin-right: 3px;
		margin-left:3px;
		padding: 5px;
		margin-bottom:3px;
		white-space: pre-wrap;
		word-wrap: break-word;
	}

	.msgString{
		margin: 0px 0px 0px 0px;
		padding: 0 3px 3px;
		width:97%;
		max-width:97%;
		max-height:45px;
		color : black;
		font-size : 15px !important;
		font-family:Tahoma !important;
		resize: none;
		border: 1px solid gray;
		border-radius: 0px;
		background: white;
		
	}
	
	.msgString:focus{
		border-color: gray !important;
		box-shadow: inset 0 0 5px gray;
		outline: medium none;
	}
	
	.sendBtn{
		display:none;
	}

	.chat_content_wrap {
		position: absolute;
		top:10%;
		left:10%;
		/*bottom:0px;*/
        margin: 0 auto;
		min-width: 200px;
        max-width: 250px;
        max-height: 325px;
        background: #cccccc;
		float:left;
		margin-right:3px;
		padding-right: 0px;
		border: 3px solid rgb(42, 118, 214);
		border-radius: 3px;
		
		-webkit-user-select: none;
		-moz-user-select: none;
    }
	
    .inputBar {
        position: relative;
		margin-top: -5px;
        /*border-top:1px solid #eee;*/
		width: 100%;
		max-width: 100%;
    }
	
	.min_max{
		width:20px;
		float:right;
		color:rgb(146, 151, 141);
		border:1px solid rgb(107, 122, 114);
		cursor:pointer;
		text-align:center;
		margin:2px;
		margin-right:1px;
		display: none;
	}
	
	.min_max:hover{
	color:rgb(199, 199, 199);
	}
	
	.cClose{
		opacity: 0.5;
		width:25px;
		float:right;
		cursor:pointer;
		text-align:center;
		margin:0px;
		padding:0px;		
		margin-left:1px;
		font-weight:bold;		
	}
	
	.cClose:hover{
		opacity: 0.9;
	}
	
	.sizeC{
		opacity: 0.5;
		width:26px;
		float:right;
		cursor:pointer;
		text-align:center;
		font-weight:bold;
		padding: 0px;
		margin-top: 0px;
		margin-right: -5px;
	}
	
	.sizeC:hover{
		opacity: 0.9;
	}
	
	.msgIcon{
		float:left;
		padding:3px;
	}
	
	.toUser_Name{
		max-width: 55%;
		float:left;
		padding: 3px;
		padding-right: 1px;
		text-overflow: ellipsis;
		white-space: nowrap;
		overflow: hidden;
	}
	
	.titleBar{
		position: relative;
		background : rgb(42, 118, 214);
		color: rgb(199, 247, 156);
		height:30px;
		width: 98%;
		max-width: 98%;
		padding-left:5px;
		font-size: 18px;
		font-family: Tahoma;
		cursor: pointer;
	}

	#confirmBox , #requestEmailforChat
	{
		display: none;
		background-color: #eee;
		border-radius: 5px;
		border: 2px solid #162369;
		position: fixed;
		width: 450px;
		left:45%;
		top:25%;
		margin-left: -150px;
		padding: 6px 8px 8px;
		box-sizing: border-box;
		text-align: center;
		font-size: 15px;
		z-index:10000;
	}
	
	#requestEmailforChat .button,#confirmBox .button {
		background-color: #ccc;
		display: inline-block;
		border-radius: 3px;
		border: 1px solid #aaa;
		padding: 2px;
		text-align: center;
		width: 80px;
		cursor: pointer;
		margin:3px;
	}
	
	#requestEmailforChat .button,#confirmBox .button:hover
	{
		background-color: #ddd;
	}
	
	#confirmBox .message
	{
		text-align: center;
		margin-bottom: 8px;
	}
	
	.timestamp
	{
		padding:0px;
		margin: 0px;
		/*margin-top: -10px;*/
		margin-bottom: -8px;
		font-size:10px;
		color:gray;
		float:right
	}
